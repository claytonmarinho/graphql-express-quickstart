  "author": "Clayton Marinho <claytonmarinho@gmail.com>",
  "license": "MIT",
  "repository": "https://github.com/claytonmarinho/graphql-express-quickstart",
  "description": "Graphql express node server kickstart project",
  "keywords": [
    "node",
    "express",
    "apollo",
    "graphql",
    "jest",
    "webpack"
  ],
  "scripts": {
    "build": "webpack --config webpack.config.js --env prod",
    "start": "webpack --config webpack.config.js --watch --env dev",
    "integration-test": "clear && yarn build && jest --runInBand --detectOpenHandles ./__tests__/*.spec.js",
    "test": "jest ./src/**/*.spec.js",
    "test:watch": "jest ./src/**/*.spec.js --watch"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "babel-loader": "^8.2.2",
    "babel-polyfill": "^6.26.0",
    "eslint": "^7.16.0",
    "eslint-plugin-jest": "^24.1.3",
    "graphql-request": "^3.4.0",
    "jest": "^26.6.3",
    "nodemon-webpack-plugin": "^4.3.2",
    "webpack": "^5.11.0",
    "webpack-cli": "^4.2.0",
    "webpack-node-externals": "^2.5.2"
  },
  "dependencies": {
    "helmet": "^4.2.0",
    "apollo-server-express": "^2.19.0",
    "dotenv": "^8.2.0"
  }
}
